  pod 'AnyThinkSigmobSDKAdapter','6.3.61'
  pod 'AnyThinkStartAppSDKAdapter','6.3.61'
  pod 'AnyThinkKidozSDKAdapter','6.3.61'
  pod 'AnyThinkFyberSDKAdapter','6.3.61'
#  pod 'AnyThinkMyTargetSDKAdapter','6.3.61'
